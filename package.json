{
  "name": "vibe-coding",
  "displayName": "vibe coding",
  "description": "Easily create code changes from LLM outputs",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vibeCodingView",
    "onCommand:vibeCoding.openView",
    "onCommand:vibeCoding.previewChanges",
    "onCommand:vibeCoding.viewDiff"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vibeCodingSidebar",
          "title": "Vibe Coding",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "vibeCodingSidebar": [
        {
          "type": "webview",
          "id": "vibeCodingView",
          "name": "Vibe Coding"
        }
      ]
    },
    "commands": [
      {
        "command": "vibeCoding.openView",
        "title": "Open Vibe Coding View"
      },
      {
        "command": "vibeCoding.viewDiff",
        "title": "View Diff"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "ignore": "^7.0.3",
    "xmldom": "^0.6.0"
  },
  "publisher": "TimvandeVathorst",
  "repository": {
    "type": "git",
    "url": "https://github.com/Timvdv/xml-to-code"
  },
  "keywords": [
    "vibe coding",
    "XML to code",
    "LLM parser"
  ]
}
